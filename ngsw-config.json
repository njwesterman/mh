{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
      {
        "name": "app",
        "installMode": "prefetch",
        "resources": {
          "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
        }
      },
      {
        "name": "assets",
        "installMode": "prefetch",
        "resources": {
          "files": ["assets/voice", "assets/sounds", "assets/data","assets/fonts","assets/speech", "/assets/sandbox/*.(eot|svg|cur|mp4|mp3|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
        }
      }
    ]
  }